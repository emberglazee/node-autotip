{
  "name": "node-autotip",
  "version": "4.5.0",
  "description": "CLI-based autotip client for Hypixel using mineflayer.",
  "author": "emberglazee",
  "license": "MIT",
  "main": "src/index.js",
  "type": "module",

  "scripts": {
    "start": "node src/index.js",
    "start:bun": "bun src/index.js",
    "start:dev": "npm run dev",

    "dev": "node src/index.js --dev",
    "dev:bun": "bun src/index.js --dev",

    "stats": "node src/index.js --stats",
    "stats:bun": "bun src/index.js --stats",

    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix"
  },

  "dependencies": {
    "axios": "^1.13.2",
    "big-integer": "^1.6.52",
    "dotenv": "^17.2.3",
    "jsonfile": "^6.2.0",
    "mineflayer": "^4.33.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/node": "18.19.130",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "eslint": "^9.39.1"
  },
  "overrides": {
    "axios": "^1.13.2"
  },

  "engines": {
    "node": ">=18",
    "bun": ">=1"
  }
}
